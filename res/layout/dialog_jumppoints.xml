<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" 
  android:layout_height="wrap_content" android:layout_width="wrap_content" >
<RelativeLayout android:id="@+id/dialog_jumppoints_LayoutDialogTitle" 
  android:layout_height="56dip" android:layout_width="wrap_content" >
	<ImageView android:id="@+id/dialog_jumppoints_icon" 
	  android:layout_height="56dip" android:layout_width="56dip" 
	  android:layout_centerVertical="true" 
	  tools:ignore="ContentDescription"
	  android:src="@drawable/button_gojump" 
	  android:scaleType="fitXY" 
	  android:padding="8dip" />
	<TextView android:id="@+id/dialog_jumppoints_title" 
	  android:layout_height="wrap_content" android:layout_width="wrap_content" 
	  android:layout_toRightOf="@+id/dialog_jumppoints_icon" 
	  android:layout_centerVertical="true" 
	  android:textAppearance="@android:style/TextAppearance.DialogWindowTitle" 
	  android:text="@string/menu_item_jumpto" />
</RelativeLayout>
<ImageButton android:id="@+id/dialog_jumppoints_ButtonGoSDcard" 
  android:layout_height="56dip" android:layout_width="56dip" 
  android:layout_alignParentRight="true" 
  tools:ignore="ContentDescription"
  android:src="@drawable/icon_sdcard" 
  android:scaleType="fitXY" 
  android:padding="8dip" />
<ImageButton android:id="@+id/dialog_jumppoints_ButtonGoRecycleBin"
  android:layout_height="56dip" android:layout_width="56dip" 
  android:layout_toLeftOf="@+id/dialog_jumppoints_ButtonGoSDcard" 
  android:layout_marginRight="12dip" 
  android:layout_marginLeft="6dip" 
  tools:ignore="ContentDescription" 
  android:src="@drawable/icon_recyclebin" 
  android:scaleType="fitXY" 
  android:padding="8dip" />
<GridView android:id="@+id/dialog_jumppoints_GridView" 
  android:layout_height="wrap_content" android:layout_width="fill_parent"
  android:layout_below="@+id/dialog_jumppoints_LayoutDialogTitle" 
  android:numColumns="auto_fit" 
  android:stretchMode="columnWidth" 
  android:columnWidth="72dip"
  android:fastScrollEnabled="true" 
  android:focusable="true" 
  android:focusableInTouchMode="true" />

</RelativeLayout>
